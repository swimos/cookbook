@web(port: 9001) {
  space: "basic"
  documentRoot: "./ui/"
  @websocket {
    serverCompressionLevel: 0
    # -1 = default; 0 = off; 1-9 = deflate level
    clientCompressionLevel: 0
    # -1 = default; 0 = off; 1-9 = deflate level
  }
}
basic: @fabric {
  @plane(class: "swim.basic.BasicPlane")
  @node {
    uri: "/unit"
    @agent(class: "swim.basic.UnitAgent")
  }
}